$TTL 604800
@   IN  SOA ns.cloud.local. admin.cloud.local. (
          5          ; Serial (RẤT QUAN TRỌNG: Đã tăng từ 4 lên 5)
          604800     ; Refresh
          86400      ; Retry
          2419200    ; Expire
          604800 )   ; Negative Cache TTL

; Name servers
@       IN  NS      ns.cloud.local.

; 
ns                               IN  A   10.10.10.10
web-frontend-server              IN  A   10.10.10.10
application-backend-server       IN  A   10.10.10.11
authentication-identity-server   IN  A   10.10.10.12
relational-database-server       IN  A   10.10.10.13

; 
app-backend.cloud.local.       IN  A   10.10.10.20  ; Yêu cầu chính
keycloak.cloud.local.          IN  A   10.10.10.12  ; Bổ sung (trỏ về IP của keycloak cũ)
minio.cloud.local.             IN  A   10.10.10.14  ; Bổ sung (tôi tạm gán IP .14, bạn có thể đổi)